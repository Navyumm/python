# ? Without return statement
# L = [1,2,3]
# print(L.append(4))                      # none
# print(L)                                # [1,2,3,4]


#! 20.1  Global Scope Vs variable scope
# *********************************************
# def g(y):
#     print(x)
#     print(x + 1)
# x = 5
# g(x)
# print(x)

# ? **********************************


# def f(y):
#     x = 1
#     x += 1
#     print(x)
# x = 5
# f(x)
# print(x)

# ? **********************************


# def h(y):
#     x += 1
# x = 5
# h(x)
